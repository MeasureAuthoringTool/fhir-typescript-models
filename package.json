    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "model-info-parser": "^0.0.6",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
